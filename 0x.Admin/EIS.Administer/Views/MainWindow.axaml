<controls:ThemeWindow xmlns="https://github.com/avaloniaui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:regions="http://prismlibrary.com/"
                 xmlns:vm="clr-namespace:EIS.Administer.ViewModels"
                 xmlns:controls="clr-namespace:EIS.Theme.Controls;assembly=EIS.Theme"
                 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                 x:Class="EIS.Administer.Views.MainWindow"
                 Icon="/Assets/avalonia-logo.ico"
                 Title="企业一体化信息平台">
    <Design.DataContext>
        <vm:MainViewModel />
    </Design.DataContext>
    <ContentControl
        Name="NavigateRegion"
        regions:RegionManager.RegionName="MainRegionContent"/>
</controls:ThemeWindow>
